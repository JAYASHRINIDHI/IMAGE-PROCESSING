   "source": [
    "import cv2\n",
    "import numpy as np\n",
    "img=cv2.imread(\"text.png\")\n",
    "cv2.imshow(\"orginal image\",img)\n",
    "\n",
    "alpha = 2.0\n",
    "beta = -160\n",
    "\n",
    "new = alpha*img+beta\n",
    "new = np.clip(new,0,255).astype(np.uint8)\n",
    "\n",
    "cv2.imshow(\"to display image\",new)\n",
    "cv2.waitKey()\n",
    "cv2.destroyAllWindows()"
   ]
